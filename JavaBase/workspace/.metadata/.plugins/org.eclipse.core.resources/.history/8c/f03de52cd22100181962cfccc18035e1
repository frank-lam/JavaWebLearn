package lesson08;

public class Demo01 {

	public static void main(String[] args) {
/*		校验qq号码. 
		1:要求必须是5-15位数字 
		2:0不能开头 
		3:必须都是数字*/
		
		String qq = "10305103051030510305";
		
		//1:要求必须是5-15位数字 
		if(qq.length() >=5 && qq.length() <= 15){
			System.out.println("qq号的长度正确 ");
		}else{
			System.out.println("qq号码的长度不正确");
		}
		

	}
}
