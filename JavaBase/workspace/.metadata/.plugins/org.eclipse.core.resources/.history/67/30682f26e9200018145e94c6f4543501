package lesson06;

public class Demo01 {

	public static void main(String[] args) throws ClassNotFoundException {
		//Class类中静态方法forName() 读取配置文件
		//1.读取info.txt文件内容
		String className = "com.gyf.model.Dog";
		
		//2.获取字节码对象
		Class clz = Class.forName(className);
		System.out.println(clz);
	}
}
